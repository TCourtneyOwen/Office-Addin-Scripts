{
  "name": "Office-Addin-Taskpane-SSO",
  "description": "",
  "author": "",
  "engines": {
    "node": ">=6.9.4"
  },
  "config": {
    "app-to-debug": "powerpoint",
    "app-type-to-debug": "desktop"
  },
  "scripts": {
    "clean": "rimraf dist",
    "tsc": "tsc -p tsconfig.json",
    "tsc-w": "tsc -p tsconfig.json --watch",
    "configure-sso": "node ./scripts/configureSSO.js",
    "cpx": "cpx \"./src/**/!(*.ts)\" dist",
    "cpx-w": "cpx \"./src/**/!(*.ts)\" dist --watch",
    "generate": "npm-run-all clean -p tsc cpx",
    "build": "rimraf lib && concurrently \"tsc -p tsconfig.json\"",
    "build:dev": "webpack --mode development --https false",
    "sideload": "office-addin-debugging start Office-Add-in-NodeJS-SSO.xml",
    "start": "concurrently \"npm run build\" \"npm run start:server\" \"npm run sideload\" ",
    "start:debug": "concurrently \"npm run build\" \"npm run start:server:debug\" \"npm run sideload\" ",
    "start:server": "nodemon dist/server.js --debug=5858",
    "start:server:debug": "nodemon --inspect dist/server.js --debug=5858",
    "watch": "webpack --mode development --watch"
  },
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.18.3",
    "commander": "^3.0.1",
    "cors": "2.8.5",
    "crypto-js": "3.1.9-1",
    "express": "4.16.4",
    "form-urlencoded": "3.0.0",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "8.4.0",
    "lodash": "4.17.13",
    "moment": "2.23.0",
    "morgan": "1.9.1",
    "node-persist": "3.0.2",
    "nodemon": "1.18.9",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.0",
    "@types/jsonwebtoken": "8.3.0",
    "@types/lodash": "4.14.112",
    "@types/moment": "^2.13.0",
    "@types/morgan": "1.7.35",
    "@types/node-persist": "0.0.33",
    "@types/uuid": "3.4.3",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.0.4",
    "cpx": "1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "office-addin-cli": "^1.0.3",
    "office-addin-debugging": "^3.0.16",
    "office-addin-dev-certs": "^1.4.10",
    "office-addin-manifest": "^1.4.12",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.2",
    "typescript": "2.9.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  }
}
