{
  "name": "office-addin-sso",
  "version": "1.4.13",
  "description": "Configure SSO for Office Addin project and provide SSO authentication service.",
  "main": "./lib/main.js",
  "scripts": {
    "build": "rimraf lib && concurrently \"tsc -p tsconfig.json\" \"npm run copy\"",
    "cli": "node lib/cli.js",
    "copy": "cpx \"src/scripts/**/*.*\" lib/scripts && cpx \"src/views/**/*.*\" lib/views",
    "test": "mocha -r ts-node/register test/**/*.ts"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-sso": "./cli.js"
  },
  "keywords": [
    "Office",
    "Office Add-in"
  ],
  "dependencies": {
    "body-parser": "1.18.3",
    "commander": "^3.0.2",
    "cookie-parser": "~1.4.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "form-urlencoded": "3.0.0",
    "http-errors": "~1.6.3",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "8.4.0",
    "lodash": "4.17.13",
    "moment": "2.23.0",
    "morgan": "1.9.1",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.0",
    "@types/jsonwebtoken": "8.3.0",
    "@types/lodash": "4.14.112",
    "@types/mocha": "^5.2.7",
    "@types/moment": "^2.13.0",
    "@types/morgan": "1.7.35",
    "@types/node-persist": "0.0.33",
    "concurrently": "^4.1.2",
    "cpx": "1.5.0",
    "dotenv": "^8.2.0",
    "mocha": "^5.2.0",
    "office-addin-cli": "^1.0.4",
    "office-addin-dev-certs": "^1.4.11",
    "office-addin-manifest": "^1.4.13",
    "office-addin-test-helpers": "1.0.14",
    "rimraf": "2.6.2",
    "ts-node": "^8.3.0",
    "typescript": "2.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  }
