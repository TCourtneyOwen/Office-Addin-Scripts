{
  "name": "office-addin-sso",
  "version": "1.4.13",
  "description": "Configure SSO for Office Addin project and provide SSO authentication service.",
  "main": "./lib/main.js",
  "scripts": {
    "build": "rimraf lib && concurrently \"npm run tsc\" \"npm run copy\"",
    "cli": "node lib/cli.js",
    "clean": "rimraf lib",
    "tsc": "tsc -p tsconfig.json",
    "tsc-w": "tsc -p tsconfig.json --watch",
    "copy": "cpx \"scripts/**/*.*\" lib/scripts",
    "start:server": "nodemon lib/server.js --debug=5858",
    "start:server:debug": "nodemon --inspect lib/server.js --debug=5858"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-manifest": "./cli.js"
  },
  "keywords": [
    "Office",
    "Office Add-in"
  ],
  "dependencies": {
    "body-parser": "1.18.3",
    "commander": "^3.0.2",
    "cors": "2.8.5",
    "express": "4.17.1",
    "form-urlencoded": "3.0.0",
    "generate-password": "^1.4.2",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "8.4.0",
    "lodash": "4.17.13",
    "moment": "2.23.0",
    "morgan": "1.9.1",
    "node-persist": "3.0.2",
    "nodemon": "1.18.9",
    "random-base64-string": "1.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.0",
    "@types/jsonwebtoken": "8.3.0",
    "@types/lodash": "4.14.112",
    "@types/moment": "^2.13.0",
    "@types/morgan": "1.7.35",
    "@types/node-persist": "0.0.33",
    "concurrently": "^4.1.2",
    "cpx": "1.5.0",
    "dotenv": "^8.2.0",
    "npm-run-all": "4.1.5",
    "office-addin-cli": "^1.0.4",
    "office-addin-dev-certs": "^1.4.11",
    "office-addin-manifest": "^1.4.13",
    "rimraf": "2.6.2",
    "typescript": "2.9.2"
  }
}
